{
    "name": "dauphin-egs-file-extension",
    "displayName": "Dauphin .egs file extension",
    "description": "Syntax highlighting etc for .egs fil",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "egs",
            "aliases": ["egs", "egs"],
            "extensions": [".dp",".egs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "egs",
            "scopeName": "source.egs",
            "path": "./syntaxes/egs.tmLanguage.json"
        }]
    }
}